@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#>.
@prefix fno: <https://w3id.org/function/ontology#>.
@prefix d2rq: <http://www.wiwiss.fu-berlin.de/suhl/bizer/D2RQ/0.1#>.
@prefix void: <http://rdfs.org/ns/void#>.
@prefix dc: <http://purl.org/dc/terms/>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix rml: <http://semweb.mmlab.be/ns/rml#>.
@prefix ql: <http://semweb.mmlab.be/ns/ql#>.
@prefix : <http://mapping.example.com/>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix org: <http://www.w3.org/ns/org#>.
@prefix vcard: <http://www.w3.org/2006/vcard/ns#>.
@prefix vivo: <http://vivoweb.org/ontology/core#>.
@prefix schema: <http://schema.org/>.
@prefix prov: <http://www.w3.org/ns/prov#>.
@prefix dcterms: <http://purl.org/dc/terms/>.
@prefix time: <http://www.w3.org/2006/time#>.
@prefix grel: <http://users.ugent.be/~bjdmeest/function/grel.ttl#>.
@prefix adms: <http://www.w3.org/ns/adms#>.
@prefix radon: <http://example.org/radon#>.
@prefix bibo: <http://purl.org/ontology/bibo/>.
@prefix rev: <http://purl.org/stuff/rev#>.

:rules_000 a void:Dataset;
    void:exampleResource :map_promotion_000.
:map_promotion_000 rml:logicalSource :source_000.
:source_000 a rml:LogicalSource;
    rml:source "data/promotions.json";
    rml:iterator "$[*]";
    rml:referenceFormulation ql:JSONPath.
:map_promotion_000 a rr:TriplesMap;
    rdfs:label "promotion".
:s_000 a rr:SubjectMap.
:map_promotion_000 rr:subjectMap :s_000.
:s_000 rr:template "http://example.org/radon#promotion_{promotionUuid}".
:pom_000 a rr:PredicateObjectMap.
:map_promotion_000 rr:predicateObjectMap :pom_000.
:pm_000 a rr:PredicateMap.
:pom_000 rr:predicateMap :pm_000.
:pm_000 rr:constant rdf:type.
:pom_000 rr:objectMap :om_000.
:om_000 a rr:ObjectMap;
    rr:constant radon:Promotion;
    rr:termType rr:IRI.
:pom_001 a rr:PredicateObjectMap.
:map_promotion_000 rr:predicateObjectMap :pom_001.
:pm_001 a rr:PredicateMap.
:pom_001 rr:predicateMap :pm_001.
:pm_001 rr:constant schema:identifier.
:pom_001 rr:objectMap :om_001.
:om_001 a rr:ObjectMap;
    rml:reference "promotionUuid";
    rr:termType rr:Literal.
:rules_000 void:exampleResource :map_dissertation_000.
:map_dissertation_000 rml:logicalSource :source_001.
:source_001 a rml:LogicalSource;
    rml:source "data/promotions.json";
    rml:iterator "$[*]";
    rml:referenceFormulation ql:JSONPath.
:map_dissertation_000 a rr:TriplesMap;
    rdfs:label "dissertation".
:s_001 a rr:SubjectMap.
:map_dissertation_000 rr:subjectMap :s_001.
:s_001 rr:template "http://example.org/radon#dissertation_{promotionUuid}".
:pom_002 a rr:PredicateObjectMap.
:map_dissertation_000 rr:predicateObjectMap :pom_002.
:pm_002 a rr:PredicateMap.
:pom_002 rr:predicateMap :pm_002.
:pm_002 rr:constant rdf:type.
:pom_002 rr:objectMap :om_002.
:om_002 a rr:ObjectMap;
    rr:constant bibo:Thesis;
    rr:termType rr:IRI.
:pom_003 a rr:PredicateObjectMap.
:map_dissertation_000 rr:predicateObjectMap :pom_003.
:pm_003 a rr:PredicateMap.
:pom_003 rr:predicateMap :pm_003.
:pm_003 rr:constant schema:identifier.
:pom_003 rr:objectMap :om_003.
:om_003 a rr:ObjectMap;
    rml:reference "promotionUuid";
    rr:termType rr:Literal.
:pom_004 a rr:PredicateObjectMap.
:map_dissertation_000 rr:predicateObjectMap :pom_004.
:pm_004 a rr:PredicateMap.
:pom_004 rr:predicateMap :pm_004.
:pm_004 rr:constant dcterms:title.
:pom_004 rr:objectMap :om_004.
:om_004 a rr:ObjectMap;
    rml:reference "dissertationData.dissertationTitle";
    rr:termType rr:Literal.
:pom_005 a rr:PredicateObjectMap.
:map_dissertation_000 rr:predicateObjectMap :pom_005.
:pm_005 a rr:PredicateMap.
:pom_005 rr:predicateMap :pm_005.
:pm_005 rr:constant dcterms:creator.
:pom_005 rr:objectMap :om_005.
:om_005 a rr:ObjectMap;
    rr:template "{personBaseData.name} {personBaseData.surname}";
    rr:termType rr:Literal.
